{
    "name": "alltvex/laravel-vehicle",
    "license": "MIT",
    "description": "Laravel package to work with vehicles data.",
    "authors": [
        {
            "name": "Alex Pettiti",
            "email": "alex.pettiti@axterisko.it",
            "homepage": "www.axterisko.it"
        }
    ],
    "minimum-stability": "dev",
    "type": "package",
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Axterisko\\Vehicle\\": "src/Vehicle/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axterisko\\Vehicle\\VehicleServiceProvider"
            ]
        }
    }
}
